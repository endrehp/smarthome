<html>
    <head>
        <link rel="stylesheet" href="/css/bootstrap.min.css" >
        <link rel="stylesheet" href="/css/style.css" >
        

    </head>
    <body>

    <%- include base.ejs %>

       
    <div class="contentContainer" >

    <% for (let i=0; i < modes.length; i++) {%>


        <div class="modeListRow">    
                
            <h3><%=modes[i].Title %></h3>
          
            <p><%=modes[i].Description %></p>
            <a href="/modes/<%= modes[i].modeID%>" class="text-muted">Read More</a></small>
            </h4>
            <button class="btn btn-primary" onclick="makeCurrent('<%=modes[i].modeID %>')">Make current</button>
            <button class="btn btn-primary" onclick="window.location.href = '/modes/modeEditor/<%= modes[i].modeID%>';">Edit</button>
            <button class="btn btn-primary" onclick="deleteMode('<%=modes[i].modeID %>')">Delete</button>
        </div>
                              
        <%} %>

        <button class="btn btn-primary" onclick="window.location.href = '/modes/modeEditor/new';">Create new mode</button>
    </div>
        

        <script src="js/modeList_script.js"></script>
        <script src="/js/jquery.slim.min.js"></script>
